<%- model_class = SurveyResponse -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:survey_template_id) %>:</strong></dt>
  <dd><%= @survey_response.survey_template_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:azienda) %>:</strong></dt>
  <dd><%= @survey_response.azienda %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
  <dd><%= @survey_response.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cognome) %>:</strong></dt>
  <dd><%= @survey_response.cognome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefono) %>:</strong></dt>
  <dd><%= @survey_response.telefono %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @survey_response.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @survey_response.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:data_compilazione) %>:</strong></dt>
  <dd><%= @survey_response.data_compilazione %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              survey_responses_path(:survey_template_id=>@survey_response.survey_template_id), :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_survey_response_path(@survey_response), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              survey_response_path(@survey_response),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
